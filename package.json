    "pg": "^8.3.3",
    "rimraf": "^3.0.2",
    "sqlite3": "^5.0.0",
    "telegraf": "^3.38.0",
    "telegraf-i18n": "^6.6.0",
    "telegraf-inline-menu": "^5.1.0",
    "telegraf-session-local": "^2.0.0",
    "telegraf-stateless-question": "^1.0.0",
    "telegram-text-entities-filler": "^0.2.0",
    "typescript": "^4.0.2"
  },
  "devDependencies": {
    "debug": "^4.2.0",
    "ts-node-dev": "^1.0.0-pre.62"
